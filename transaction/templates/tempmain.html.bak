{% load static %}
<!DOCTYPE html>
<html>
   <head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <title>Auvit</title>
      <!-- Tell the browser to be responsive to screen width -->
      <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
      <!-- Bootstrap 3.3.6 -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/bootstrap/dist/css/bootstrap.min.css' %}" />
      <!-- Font Awesome -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/font-awesome/css/font-awesome.min.css' %}" />
      <!-- Ionicons -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/Ionicons/css/ionicons.min.css' %}" />
      <!-- Theme style -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/dist/css/AdminLTE.min.css' %}" />
      <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/dist/css/skins/_all-skins.min.css' %}" />
      <!-- iCheck -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/plugins/iCheck/flat/blue.css' %}" />
      <!-- Morris chart -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/morris.js/morris.css' %}" />
      <!-- jvectormap -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/jvectormap/jquery-jvectormap.css' %}" />
      <!-- Date Picker -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css' %}" />
      <!-- Daterange picker -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/bootstrap-daterangepicker/daterangepicker.css' %}" />
      <!-- bootstrap wysihtml5 - text editor -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css' %}" />
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css' %}" />
      <!-- own css -->
      <link rel="stylesheet" type="text/css" href="{% static 'maincss/custom/custom.css' %}" />
      <!-- iCheck for checkboxes and radio inputs -->
      <link rel="stylesheet" href="{% static 'maincss/plugins/iCheck/all.css' %}" />
      <!-- Select2 -->
      <link rel="stylesheet" href="{% static 'maincss/bower_components/select2/dist/css/select2.min.css' %}">
      <!-- Google Font --> 
      <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
   </head>
   <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
   <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
   <!--[if lt IE 9]>
   <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
   <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
   <![endif]-->
   </head>
   <body class="hold-transition skin-blue sidebar-mini">
      <div class="wrapper">
      <header class="main-header">
         <!-- Logo -->
         <a href="/transaction/transmain/" class="logo">
            <!-- mini logo for sidebar mini 50x50 pixels -->
            <span class="logo-mini"><b>A</b>uvit</span>
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg"><b>A</b>uvit</span>
         </a>
         <!-- Header Navbar: style can be found in header.less -->
         <nav class="navbar navbar-static-top">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
            </a>
            {% block navbarRight %}
            <div class="navbar-custom-menu">
               <ul class="nav navbar-nav">
                  <li>
                     <a href="#" data-toggle="control-sidebar">{{element.project_id.title}}</a>
                  </li>
                  <li class="dropdown notifications-menu">
                     <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        Switch Project
                        <ul class="dropdown-menu">
                           <!-- inner menu: contains the actual data -->
                           <li class="dropdown-item" style="height:45px;padding: 10px;text-align: center;">
                              <h4>Avaliable Projects</h4>
                           </li>
                           {% if projectselect %}
                           {% for selection in projectselect %}
                           <li class="header">
                     <a href="/transaction/switchproject/{{selection.project_id_id}}">
                     <h4>
                     <img src="{% static 'maincss/img/folder.png' %}" class="img-circle" alt="User Image" />
                     &nbsp;  &nbsp;&nbsp;{{selection.project_id.title}}
                     </h4>
                     </a>
                     </li>
                     {% endfor %}
                     {% endif %}
                     </li>
                     </ul>
                  </a>
                  </li>
                  <!-- User Account: style can be found in dropdown.less -->
                  <li class="dropdown user user-menu">
                     <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                     <img src="{% static 'maincss/images/user.png' %}" style="background: white;width:20px;height:20px;" class="img-circle" alt="User Image">
                     <span class="hidden-xs">{{element.userid}}</span>
                     </a>
                     <ul class="dropdown-menu">
                        <!-- User image -->
                        <li class="user-header">
                           <img src="{% static 'maincss/images/user.png' %}" style="background: white" class="img-circle" alt="User Image">
                           <p>
                              {{element.userid}}
                           </p>
                        </li>
                        <!-- Menu Footer-->
                        <li class="user-footer">
                           <div class="pull-right">
                              <a href="{% url 'signout' %}" class="btn btn-default btn-flat" style="background: none;border: none;"><button type="button" class="btn btn-block btn-danger">Sign Out</button></a>
                           </div>
                        </li>
                     </ul>
                  </li>
                  <!-- Control Sidebar Toggle Button -->
               </ul>
            </div>
            {% endblock navbarRight %}
         </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside class="main-sidebar">
         <!-- sidebar: style can be found in sidebar.less -->
         <section class="sidebar">
            {% block sidemenu  %}
            <!-- sidebar menu: : style can be found in sidebar.less -->
            <ul class="sidebar-menu" data-widget="tree">
               <li class="active treeview">
                  <a href="#">
                  <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                  <span class="pull-right-container">
                  </span>
                  </a>
               </li>
                <li class="treeview">
                  <a href="/rolesetup/roleindex">
                    <i class="fa fa-cogs"></i> <span>Role Setup</span>
                    <span class="pull-right-container">
                    </span>
                  </a>
                </li >
                
                 <li class="treeview">
                  <a href="/transaction/enumlist/">
                     <i class="fa fa-map-o"></i> <span>Enum List</span>
                     <span class="pull-right-container">
                        <!--    <i class="fa fa-angle-left pull-right"></i> -->
                     </span>
                  </a>
               </li>
               
               <li class="treeview">
                  <a href="#">
                  <i class="fa fa-pencil-square-o"></i> 
                  <span>Login</span>
                  <span class="pull-right-container">
                  <i class="fa fa-angle-left pull-right"></i> 
                  </span>
                  </a>
                  <ul class="treeview-menu">
                     <li><a href="/logintemplate/loginindex/"><i class="fa fa-circle-o"></i> Design login Page</a></li>
                     <li><a href="/logintemplate/serverconfig/"><i class="fa fa-circle-o"></i> Server Config</a></li>
                  </ul>
               </li>
               <li class="treeview">
                  <a href="/hometemplate/pagecomponent/">
                     <i class="fa fa-sticky-note-o"></i> <span>HomePage</span>
                     <span class="pull-right-container">
                        <!--    <i class="fa fa-angle-left pull-right"></i> -->
                     </span>
                  </a>
               </li>
                <li class="treeview">
                  <a href="{% url 'transindex' %}">
                     <i class="fa fa-file-text"></i> <span>Transaction</span>
                     <span class="pull-right-container">
                        <!--    <i class="fa fa-angle-left pull-right"></i> -->
                     </span>
                  </a>
               </li>
                <li class="treeview">
                  <a href="/reportview/repoviewdetails/">
                      <i class="fa fa-desktop"></i> <span>ReportView</span>
                  </a>
                </li>
                <li class="treeview">
                  <a href="/syncmaster/configurations/">
                     <i class="fa fa-refresh"></i> <span>Sync Configurations</span>
                     <span class="pull-right-container">
                        <!--    <i class="fa fa-angle-left pull-right"></i> -->
                     </span>
                  </a>
               </li>
               <li class="treeview">
                  <a href="/smssetup/smsindex/">
                      <i class="fa fa-comment-o"></i> <span>SMS Configuration</span>
                  </a>
              </li>
              <li class="treeview">
                <a href="/notification/notificationindex/">
                   <i class="fa fa-bell-o"></i> <span>Notification Configuration</span>
                   <span class="pull-right-container">
                      <!--    <i class="fa fa-angle-left pull-right"></i> -->
                   </span>
                </a>
             </li>
              
                
            </ul>
            {% endblock sidemenu %}
         </section>
         <!-- /.sidebar -->
      </aside>
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
         <!-- Content Header (Page header) -->
         {% block maincontent %}
         <section class="content-header">
            <h1>
               Dashboard
               <small>Control panel</small>
            </h1>
            <ol class="breadcrumb">
               <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
               <li class="active">Dashboard</li>
            </ol>
            {{project_title}}
         </section>
         {% endblock maincontent %}
      </div>
      <!-- /.content-wrapper -->
      <!-- /.control-sidebar -->
      <!-- Add the sidebar's background. This div must be placed
         immediately after the control sidebar -->
      <div class="control-sidebar-bg"></div>
      <!-- ./wrapper -->
      <!-- jQuery 2.2.3 -->
      <script src="{% static 'maincss/bower_components/jquery/dist/jquery.min.js' %}"></script>
      <!-- jQuery UI 1.11.4 -->
      <script src="{% static 'maincss/bower_components/jquery-ui/jquery-ui.min.js' %}"></script>
      <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
      <script>
         $.widget.bridge('uibutton', $.ui.button);
      </script>
      <!-- Bootstrap 3.3.6 -->
      <script src="{% static 'maincss/bower_components/bootstrap/dist/js/bootstrap.min.js' %}"></script>
      <!-- Morris.js charts -->
      <script src="{% static 'maincss/bower_components/raphael/raphael.min.js' %}"></script>
      <script src="{% static 'maincss/bower_components/morris.js/morris.min.js' %}"></script>
      <!-- Sparkline -->
      <script src="{% static 'maincss/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js' %}"></script>
      <!-- jvectormap -->
      <script src="{% static 'maincss/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js' %}"></script>
      <script src="{% static 'maincss/plugins/jvectormap/jquery-jvectormap-world-mill-en.js' %}"></script>
      <!-- jQuery Knob Chart -->
      <script src="{% static 'maincss/bower_components/jquery-knob/dist/jquery.knob.min.js' %}"></script>
      <!-- daterangepicker -->
      <script src="{% static 'maincss/bower_components/moment/min/moment.min.js' %}"></script>
      <script src="{% static 'maincss/bower_components/bootstrap-daterangepicker/daterangepicker.js' %}"></script>
      <!-- datepicker -->
      <script src="{% static 'maincss/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js' %}"></script>
      <!-- Bootstrap WYSIHTML5 -->
      <script src="{% static 'maincss/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js' %}"></script>
      <!-- Slimscroll -->
      <script src="{% static 'maincss/bower_components/jquery-slimscroll/jquery.slimscroll.min.js' %}"></script>
      <!-- FastClick -->
      <script src="{% static 'maincss/bower_components/fastclick/lib/fastclick.js' %}"></script>
      <!-- iCheck 1.0.1 -->
      <script src="{% static 'maincss/plugins/iCheck/icheck.min.js' %}"></script>
      <!-- AdminLTE App -->
      <script src="{% static 'maincss/dist/js/app.min.js' %}"></script>
      <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
      <script src="{% static 'maincss/dist/js/pages/dashboard.js' %}"></script>
      <!-- AdminLTE for demo purposes -->
      <script src="{% static 'maincss/bower_components/datatables.net/js/jquery.dataTables.min.js' %}"></script>
      <script src="{% static 'maincss/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js' %}"></script>
      <!-- DataTables -->
      <script src="{% static 'maincss/bower_components/select2/dist/js/select2.full.min.js' %}"></script>
      <!--Select2 -->
        <!-- <script src="{% static 'maincss/js/form/formset.min.js' %}"></script>  -->
      <!--Formset JS -->
      <!-- <script src="{% static 'maincss/js/form/formset_count.js' %}"></script> -->
      <!--Formset Count JS -->
       <script src="{% static 'maincss/js/form/jquery.formset.js' %}"></script> 
 </div>
   </body>
</html>